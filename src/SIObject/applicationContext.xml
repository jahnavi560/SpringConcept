<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="AddressBean" class="SIObject.Address">  
<property name="city" value="Anjar"></property>
<property name="state" value="Gujarat"></property>
<property name="country" value="India"></property>
</bean>  
  
<bean id="EmployeeBean" class="SIObject.Employee">  
<property name="id" value="10"></property>
<property name="name" value="jahnavi"></property>
<property name="address" ref="AddressBean"></property>
</bean>  
  
</beans>  
